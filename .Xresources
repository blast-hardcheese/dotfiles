Xcursor.theme: Adwaita
Xcursor.size: 16

Xft*antialias: true
Xft*hinting:   true
Xft*hintstyle: hintfull
! Xft*rgba:      rgb

URxvt*background: Black
URxvt*foreground: White
URxvt*geometry: 80x24
URxvt*scrollBar: false

URxvt*perl-lib: /home/blast/.urxvt/ext
URxvt*perl-ext-common: font-size,eval,matcher,selection-to-clipboard,xim-onthespot

URxvt*keysym.C-equal: font-size:reset
! Man pages suggest these should work, but...
! URxvt*keysym.C-Up: builtin:
! URxvt*keysym.C-Down: builtin:
! URxvt*keysym.C-S-Up: builtin:
! URxvt*keysym.C-S-Down: builtin:

! Other colors
URxvt*color0: black
URxvt*color1: red3
URxvt*color2: green3
URxvt*color3: yellow3
URxvt*color4: blue2
URxvt*color5: magenta3
URxvt*color6: cyan3
URxvt*color7: gray90
URxvt*color8: grey50
URxvt*color9: red
URxvt*color10: green
URxvt*color11: yellow
URxvt*color12: rgb:5c/5c/ff
URxvt*color13: magenta
URxvt*color14: cyan
URxvt*color15: white

URxvt.iso14755: false
URxvt.iso14755_52: false

URxvt.font: -misc-fixed-medium-r-semicondensed--13-120-75-75-c-60-iso10646-1

! URxvt.font:           xft:fixed:pixelsize=13
! URxvt.boldFont:       xft:fixed:bold:pixelsize=13
! URxvt.italicFont:     xft:fixed:italic:pixelsize=13
! URxvt.boldItalicFont: xft:fixed:bold:italic:pixelsize=13
